{
  "name": "@ray-js/lamp-rhythm-circle",
  "version": "0.0.10",
  "description": "照明生物节律色环",
  "main": "lib/index",
  "files": [
    "lib"
  ],
  "license": "MIT",
  "types": "lib/index.d.ts",
  "maintainers": [
    "tuya_npm",
    {
      "name": "tuyafe",
      "email": "tuyafe@tuya.com"
    }
  ],
  "scripts": {
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "build": "ray build --type=component",
    "watch": "ray start --type=component",
    "build:tuya": "ray build ./example --target=tuya",
    "build:wechat": "ray build ./example --target=wechat",
    "build:web": "ray build ./example --target=web",
    "build:native": "ray build ./example --target=native",
    "start:native": "ray start ./example -t native --verbose",
    "start:tuya": "ray start ./example -t tuya --verbose",
    "start:wechat": "ray start ./example -t wechat --verbose",
    "start:web": "ray start ./example -t web",
    "prepublishOnly": "yarn build",
    "release-it": "standard-version"
  },
  "peerDependencies": {
    "@ray-js/panel-sdk": "^1.1.4"
  },
  "devDependencies": {
    "@luozhu/eslint-config-typescript": "^1.9.0",
    "@luozhu/prettier-config": "^1.2.0",
    "@ray-js/cli": "^1.7.18",
    "@ray-js/ray": "^1.7.18",
    "@types/fs-extra": "^9.0.12",
    "@types/inquirer": "^7.3.3",
    "@types/ora": "^3.2.0",
    "eslint": "^8.4.1",
    "husky": "^8.0.1",
    "prettier": "^2.5.1",
    "standard-version": "^9.3.2",
    "typescript": "^4.5.4"
  },
  "resolutions": {
    "shell-quote": "~1.7.3",
    "follow-redirects": "~1.15.6"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS --config commitlint.config.js",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "ahooks": "^3.8.5"
  }
}