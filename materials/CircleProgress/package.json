{
  "name": "@ray-js/circle-progress",
  "version": "0.1.4",
  "description": "通用环形进度条",
  "main": "lib/index",
  "files": [
    "lib"
  ],
  "license": "MIT",
  "types": "lib/index.d.ts",
  "maintainers": [
    "tuya_npm",
    {
      "name": "tuyafe",
      "email": "tuyafe@tuya.com"
    }
  ],
  "scripts": {
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "build": "ray build --type=component",
    "watch": "ray start --type=component --output ./example/src/lib",
    "dev": "yarn start:tuya",
    "build:tuya": "ray build ./example",
    "start:tuya": "ray start ./example",
    "prepublishOnly": "yarn build",
    "release-it": "standard-version",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
  "peerDependencies": {
    "@ray-js/ray": "^1.4.9"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@ray-js/cli": "^1.4.9",
    "@ray-js/panel-sdk": "^1.13.1",
    "@ray-js/ray": "^1.4.9",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^22.9.0",
    "core-js": "^3.19.1",
    "eslint-config-tuya-panel": "^0.4.2",
    "husky": "^1.2.0",
    "jest": "^30.2.0",
    "jsdom": "^27.1.0",
    "lint-staged": "^10.2.11",
    "standard-version": "9.3.2",
    "ts-jest": "^29.4.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS --config commitlint.config.js",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "罗丘（黄辉）"
}