.ray_miniapp_markdown__latex--line {
  margin: 4rpx 8rpx;
  vertical-align: middle;
}

.ray_miniapp_markdown__latex--block {
  display: block;
  margin: 1em auto;
}

.ray_miniapp_markdown__yuml {
  display: block;
}

.ray_miniapp_markdown__yumlBox {
  width: 100%;
  overflow-x: auto;
}

.ray_miniapp_markdown__yumlView {
  margin: 0 auto;
  padding-bottom: 40rpx;
}

/**代码行号**/
.ray_miniapp_markdown__lineNum {
  text-align: right;
  float: left;
  padding: 0;
  margin: 0 1em 0 0;
}

.ray_miniapp_markdown__lineNumLine {
  list-style: none;
}
